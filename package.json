{
  "name": "fifteen-puzzle",
  "private": true,
  "version": "0.1.1",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "pretty": "prettier --write src/**/*.{ts,tsx,pcss}",
    "style": "stylelint **/*.{css,pcss,scss,less}",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives",
    "format": "yarn pretty && yarn style --fix && yarn lint --fix",
    "preview": "vite preview",
    "deploy": "yarn build && gh-pages -b deployment -m 'build: Deployment' -d dist"
  },
  "devDependencies": {
    "@arrow-js/core": "^1.0.0-alpha.9",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "gh-pages": "^5.0.0",
    "postcss": "^8.4.24",
    "postcss-advanced-variables": "^3.0.1",
    "postcss-extend": "^1.0.5",
    "postcss-nesting": "^12.0.0",
    "postcss-preset-env": "^9.0.0",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "stylelint": "^15.9.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.6",
    "vite": "^4.3.9"
  }
}
